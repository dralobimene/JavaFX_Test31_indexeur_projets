	prefWidth="600.0"
	xmlns="http://javafx.com/javafx/18"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="applicationShowDate.ControlleurClass">
   <children>
      <Pane
      	layoutX="-99.0"
      	layoutY="-129.0"
      	prefHeight="600.0"
      	prefWidth="600.0"
